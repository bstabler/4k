String1 = mf2 *  mf8
# String String2: string parameter String2
String2 : YDescription
String2[] = Frequency
String2[] = Frequency
String2[] = No. of trips
# Expression Expression0: O-D pair expression for data value 1 on X-axis (tr,o-d)
Expression0 : (0x0,3) XValue1/
Expression0[] = [non-hbw sov generalized cost] mf2
Expression0[] = [e.g. times scenario 1] mf
# Expression Expression1: O-D pair expression for weight value 1 on Y-axis (tr,o-d)
Expression1 : (0x0,3) YValue1/
Expression1[] = [nhb other/other survey records] mf9
Expression1[] = [number of O-D pairs] 1
# Expression Expression2: O-D pair expression for data value 2 on X-axis (tr,o-d)
Expression2 : (0x0,3) XValue2/
Expression2[] = [non-hbw sov generalized cost] mf2
Expression2[] = [e.g. times scenario 2] mf
# Expression Expression3: O-D pair expression for weight value 2 on Y-axis (tr,o-d)
Expression3 : (0x0,3) YValue2/
Expression3[] = [nhb other/other distribution] mf8
Expression3[] = [number of O-D pairs] 1
# Filter Filter0: expression for O-D pair filter 1 (tr,o-d,unbuffered)
Filter0 : Filter1/
Filter0[] = p<3701&&q<3701
Filter0[] = [all O-D pairs] 1
Filter0[] = [e.g. more than 10 trips] mf1>10
# Filter Filter1: expression for O-D pair filter 2 (tr,o-d,unbuffered)
Filter1 : Filter2/
Filter1[] = p<3701&&q<3701
Filter1[] = [all O-D pairs] 1
Filter1[] = [e.g. more than 10 trips] mf1>10

[Parameter conversion]
# String Name: layer name that appears on layer control tab (tr,subst)
Name = Default Legend 2
# Flags Flags: bit pattern containing layer flags
Flags = 0xbfb0000
# Bool CFlag: display layer controls
CFlag = 0
# String SA: string parameter SA (subst)
SA : /XValue2
SA[] = [non-hbw sov generalized cost] mf2
SA[] = [e.g. times scenario 2] mf
# String SB: string parameter SB (subst)
SB : /YValue2
SB[] = [nhb other/other distribution] mf8
SB[] = [number of O-D pairs] 1
# Expression Expression: expression for parameter conversion (tr,string,max. 12 values)
Expression = strip(uncomment(SA))+if(strip(uncomment(SB))>""&&strip(uncomment(SB))!="1"," * "+uncomment(SB),"")
# String SR: string result
SR : DefaultLegend2
SR = mf2 *  mf8

[Parameter conversion]
# String Name: layer name that appears on layer control tab (tr,subst)
Name = Default Legend 1
# Flags Flags: bit pattern containing layer flags
Flags = 0xbfb0000
# Bool CFlag: display layer controls
CFlag = 0
# String SA: string parameter SA (subst)
SA : /XValue1
SA[] = [non-hbw sov generalized cost] mf2
SA[] = [e.g. times scenario 1] mf
# Box BA: box parameter BA
BA = 0;0;0;0
# String SB: string parameter SB (subst)
SB : /YValue1
SB[] = [nhb other/other survey records] mf9
SB[] = [number of O-D pairs] 1
# Box BB: box parameter BB
BB = 0;0;0;0
# Box BC: box parameter BC
BC = 0;0;0;0
# Box BD: box parameter BD
BD = 0;0;0;0
# Box BE: box parameter BE
BE = 0;0;0;0
# Expression Expression: expression for parameter conversion (tr,string,max. 12 values)
Expression = strip(uncomment(SA))+if(strip(uncomment(SB))>""&&strip(uncomment(SB))!="1"," * "+uncomment(SB),"")
# String SR: string result
SR : DefaultLegend1
SR = mf2 *  mf9
# Box BR: box result
BR = 0;0;0;0

[Legend]
# Flags Flags: bit pattern containing layer flags
Flags = 0x1f00000
# Bool CFlag: display layer controls
CFlag = 0
# Bool SFlag: display layer on screen
SFlag : /ShowChart
SFlag = 1
# Bool MFlag: display layer under the magnifier
MFlag : /ShowChart
MFlag = 1
# Bool LFlag: include layer in the legend window
LFlag = 0
# Bool IFlag: allow interactive input
IFlag = 0
# Bool BFlag: include bounding box in full view
BFlag = 1
# Float TextSize: text size for scales and symbols
TextSize = 10
# Float TextXPos: horizontal position of text anchor
TextXPos = 0.890747
# Float TextYPos: vertical position of text anchor
TextYPos = 0.104762
# Float GraphicStyleIncrement: style index increment used for graphic part of item
GraphicStyleIncrement = 1
# Integer TextLayout: layout definition
TextLayout = 397332
# Text TextString: text that appears in item (tr,subst)
TextString = %<String0:StyleLegend1>%\
%<String1:StyleLegend2>%
# Style SymbolStyle: style used for graphic part of items
SymbolStyle : BarStyle
SymbolStyle[] = 0x20b12d16 0x0dd1998f 0x00000000 0x05
SymbolStyle[] = 0x20007f00 0x01c9e68c 0x00000000 0x05

